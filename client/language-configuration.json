{
    "wordPattern": "([A-Za-z0-9_][A-Za-z0-9_\\-\\.]*[A-Za-z0-9_]|[A-Za-z0-9_])",
    "comments": {
      "lineComment": "#"
    },
    "brackets": [
      ["{", "}"],
      ["[", "]"],
      ["(", ")"]
    ],
    "autoClosingPairs": [
